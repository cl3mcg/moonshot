            <a href="<% if (preadviseTender) { %>/preadvise/<%= preadviseTender.id %><% } else { %>/register/start<% } %>" class="btn btn-outline-danger border-danger"><i class="bi bi-x-circle-fill me-2"></i>Abort</a>
        </div>
      </div>
    </div>
  </div>
</div>
